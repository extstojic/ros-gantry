<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="gantry_world">
    
    <!-- Physics -->
    <physics type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
    </physics>
    
    <!-- Lighting -->
    <include>
      <uri>model://sun</uri>
    </include>
    
    <!-- Ground -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- Work Table -->
    <model name="work_table">
      <static>true</static>
      <pose>0 0 0.45 0 0 0</pose>
      <link name="table_link">
        <visual name="visual">
          <geometry>
            <box><size>2.0 1.5 0.1</size></box>
          </geometry>
          <material>
            <ambient>0.6 0.4 0.2 1</ambient>
            <diffuse>0.6 0.4 0.2 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box><size>2.0 1.5 0.1</size></box>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- GANTRY ROBOT -->
    <model name="gantry_robot">
      <pose>0 0 0.5 0 0 0</pose>
      
      <!-- Base (fixed to world) -->
      <link name="base_link">
        <pose>0 0 0 0 0 0</pose>
        <inertial>
          <mass>100</mass>
          <inertia>
            <ixx>10</ixx><iyy>10</iyy><izz>10</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="base_visual">
          <geometry>
            <box><size>1.0 1.0 0.2</size></box>
          </geometry>
          <material>
            <ambient>0.3 0.3 0.3 1</ambient>
            <diffuse>0.5 0.5 0.5 1</diffuse>
          </material>
        </visual>
        <collision name="base_collision">
          <geometry>
            <box><size>1.0 1.0 0.2</size></box>
          </geometry>
        </collision>
      </link>
      
      <!-- X-axis rail -->
      <link name="x_rail">
        <pose>0 0 0.15 0 0 0</pose>
        <inertial>
          <mass>5</mass>
          <inertia>
            <ixx>0.5</ixx><iyy>0.5</iyy><izz>0.5</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="x_rail_visual">
          <geometry>
            <box><size>1.2 0.08 0.08</size></box>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.3 0.3 0.3 1</diffuse>
          </material>
        </visual>
      </link>
      
      <!-- X Carriage (moves along X) -->
      <link name="x_carriage">
        <pose>0 0 0.25 0 0 0</pose>
        <inertial>
          <mass>20</mass>
          <inertia>
            <ixx>1</ixx><iyy>1</iyy><izz>1</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="x_carriage_visual">
          <geometry>
            <box><size>0.25 0.5 0.15</size></box>
          </geometry>
          <material>
            <ambient>1.0 0.5 0.0 1</ambient>
            <diffuse>1.0 0.647 0.0 1</diffuse>
          </material>
        </visual>
        <collision name="x_carriage_collision">
          <geometry>
            <box><size>0.25 0.5 0.15</size></box>
          </geometry>
        </collision>
      </link>
      
      <!-- Y Carriage (moves along Y) -->
      <link name="y_carriage">
        <pose>0 0 0.35 0 0 0</pose>
        <inertial>
          <mass>15</mass>
          <inertia>
            <ixx>1</ixx><iyy>1</iyy><izz>1</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="y_carriage_visual">
          <geometry>
            <box><size>0.5 0.2 0.12</size></box>
          </geometry>
          <material>
            <ambient>0.0 0.0 1.0 1</ambient>
            <diffuse>0.2 0.2 1.0 1</diffuse>
          </material>
        </visual>
        <collision name="y_carriage_collision">
          <geometry>
            <box><size>0.5 0.2 0.12</size></box>
          </geometry>
        </collision>
      </link>
      
      <!-- Z Carriage (moves along Z) -->
      <link name="z_carriage">
        <pose>0 0 0.5 0 0 0</pose>
        <inertial>
          <mass>10</mass>
          <inertia>
            <ixx>0.5</ixx><iyy>0.5</iyy><izz>0.5</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="z_carriage_visual">
          <geometry>
            <box><size>0.15 0.15 0.25</size></box>
          </geometry>
          <material>
            <ambient>0.0 1.0 0.0 1</ambient>
            <diffuse>0.2 1.0 0.2 1</diffuse>
          </material>
        </visual>
        <collision name="z_carriage_collision">
          <geometry>
            <box><size>0.15 0.15 0.25</size></box>
          </geometry>
        </collision>
      </link>
      
      <!-- End Effector -->
      <link name="end_effector">
        <pose>0 0 0.35 0 0 0</pose>
        <inertial>
          <mass>2</mass>
          <inertia>
            <ixx>0.1</ixx><iyy>0.1</iyy><izz>0.1</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="tool_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.04</radius><length>0.1</length></cylinder>
          </geometry>
          <material>
            <ambient>1.0 0.0 0.0 1</ambient>
            <diffuse>1.0 0.2 0.2 1</diffuse>
          </material>
        </visual>
        <visual name="gripper_visual">
          <pose>0 0 -0.08 0 0 0</pose>
          <geometry>
            <cylinder><radius>0.06</radius><length>0.02</length></cylinder>
          </geometry>
          <material>
            <ambient>1.0 1.0 0.0 1</ambient>
            <diffuse>1.0 1.0 0.2 1</diffuse>
          </material>
        </visual>
      </link>
      
      <!-- JOINTS -->
      
      <!-- Fix base to world -->
      <joint name="world_joint" type="fixed">
        <parent>world</parent>
        <child>base_link</child>
      </joint>
      
      <!-- Fix rail to base -->
      <joint name="rail_joint" type="fixed">
        <parent>base_link</parent>
        <child>x_rail</child>
      </joint>
      
      <!-- X axis (prismatic) -->
      <joint name="joint_x" type="prismatic">
        <parent>base_link</parent>
        <child>x_carriage</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-0.4</lower>
            <upper>0.4</upper>
            <effort>100</effort>
            <velocity>1.0</velocity>
          </limit>
          <dynamics>
            <damping>10</damping>
            <friction>5</friction>
          </dynamics>
        </axis>
      </joint>
      
      <!-- Y axis (prismatic) -->
      <joint name="joint_y" type="prismatic">
        <parent>x_carriage</parent>
        <child>y_carriage</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-0.3</lower>
            <upper>0.3</upper>
            <effort>100</effort>
            <velocity>1.0</velocity>
          </limit>
          <dynamics>
            <damping>10</damping>
            <friction>5</friction>
          </dynamics>
        </axis>
      </joint>
      
      <!-- Z axis (prismatic) -->
      <joint name="joint_z" type="prismatic">
        <parent>y_carriage</parent>
        <child>z_carriage</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-0.5</lower>
            <upper>0.5</upper>
            <effort>100</effort>
            <velocity>1.0</velocity>
          </limit>
          <dynamics>
            <damping>10</damping>
            <friction>5</friction>
          </dynamics>
        </axis>
      </joint>
      
      <!-- End effector (fixed to z_carriage) -->
      <joint name="tool_joint" type="fixed">
        <parent>z_carriage</parent>
        <child>end_effector</child>
        <pose>0 0 -0.2 0 0 0</pose>
      </joint>
      
    </model>
    
    <!-- Sample objects for pick and place -->
    <model name="box_1">
      <pose>0.3 0.2 0.55 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><iyy>0.001</iyy><izz>0.001</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="visual">
          <geometry>
            <box><size>0.05 0.05 0.05</size></box>
          </geometry>
          <material>
            <ambient>0.0 0.0 1.0 1</ambient>
            <diffuse>0.0 0.2 1.0 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box><size>0.05 0.05 0.05</size></box>
          </geometry>
        </collision>
      </link>
    </model>
    
    <model name="box_2">
      <pose>-0.3 0.2 0.55 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx><iyy>0.001</iyy><izz>0.001</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="visual">
          <geometry>
            <box><size>0.05 0.05 0.05</size></box>
          </geometry>
          <material>
            <ambient>0.0 1.0 0.0 1</ambient>
            <diffuse>0.2 1.0 0.2 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box><size>0.05 0.05 0.05</size></box>
          </geometry>
        </collision>
      </link>
    </model>
    
    <model name="cylinder_1">
      <pose>0.0 -0.2 0.55 0 0 0</pose>
      <link name="link">
        <inertial>
          <mass>0.3</mass>
          <inertia>
            <ixx>0.001</ixx><iyy>0.001</iyy><izz>0.001</izz>
            <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="visual">
          <geometry>
            <cylinder><radius>0.025</radius><length>0.06</length></cylinder>
          </geometry>
          <material>
            <ambient>1.0 0.0 0.0 1</ambient>
            <diffuse>1.0 0.2 0.2 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <cylinder><radius>0.025</radius><length>0.06</length></cylinder>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- Camera position -->
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>2.5 -2.5 2.0 0 0.4 2.35</pose>
        <view_controller>orbit</view_controller>
      </camera>
    </gui>
    
  </world>
</sdf>
